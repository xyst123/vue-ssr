{
  "presets": [
    // 包含@babel/preset-es2015，@babel/preset-es2016，@babel/preset-es2017
    "@babel/preset-env"
  ],
  "plugins": [
    // 避免多次编译出helper函数；解决@babel/polyfill提供的类或者实例方法污染全局作用域的问题；
    "@babel/plugin-transform-runtime",
    // 解析class的属性
    "@babel/plugin-proposal-class-properties"
  ]
}