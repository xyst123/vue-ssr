{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "logger": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-promise-reject-errors": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "no-return-assign": 0,
    "global-require": 0,
    "no-console": 0,
    "import/no-dynamic-require": 0
  }
}